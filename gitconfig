[user]
    name = !!name!!
    email = !!email!!
[color]
    diff = always
    grep = always
    status = always
[alias]
    st = status
    s = status
    c = commit
    commmit = commit
    pul = pull
    rbm = rebase origin/master
    a = add
    g = grep
    d = diff
    dh = diff HEAD
    dc = diff --cached
    l = log
    ll = log --format='tformat:%C(yellow)%h%Creset %C(bold)%aN%Creset %C(cyan)%s %b%Creset (%cr)'
    pnp = !git fetch && git rebase origin/master && git push
